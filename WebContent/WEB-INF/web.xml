<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">

	<resource-ref>
		<description>ConnectionPool</description>
		<res-ref-name>jdbc/myOracle</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>

	<servlet>
		<servlet-name>VisitInsert</servlet-name>
		<servlet-class>bbs.VisitInsert</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>VisitInsert</servlet-name>
		<url-pattern>/VisitInsert</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>VisitList</servlet-name>
		<servlet-class>bbs.VisitList</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>VisitList</servlet-name>
		<url-pattern>/VisitList</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>PieChartServlet</servlet-name>
		<servlet-class>chart.PieChartServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PieChartServlet</servlet-name>
		<url-pattern>/PieChartServlet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>XYPlotServlet</servlet-name>
		<servlet-class>chart.XYPlotServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>XYPlotServlet</servlet-name>
		<url-pattern>/XYPlotServlet</url-pattern>
	</servlet-mapping>

	<jsp-config>
		<taglib>
			<taglib-uri>/WEBâ€”INF/tlds/el-functions.tld</taglib-uri>
			<taglib-location>/WEB-INF/tlds/el-functions.tld</taglib-location>
		</taglib>
	</jsp-config>

	<!-- <servlet> <servlet-name>controlOne</servlet-name> <servlet-class>mvc.control.ControlServlet</servlet-class> 
		</servlet> <servlet-mapping> <servlet-name>controlOne</servlet-name> <url-pattern>*.do</url-pattern> 
		</servlet-mapping> -->

	<servlet>
		<servlet-name>myControl</servlet-name>
		<servlet-class>mvcMem.control.ControlServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>myControl</servlet-name>
		<url-pattern>*.mdo</url-pattern>
	</servlet-mapping>

	<!-- <servlet> <servlet-name>ControllerURI</servlet-name> <servlet-class>mvc.Controller</servlet-class> 
		<init-param> <param-name>propertyConfig</param-name> <param-value>Command.properties</param-value> 
		</init-param> </servlet> <servlet-mapping> <servlet-name>ControllerURI</servlet-name> 
		<url-pattern>*.do</url-pattern> </servlet-mapping> -->

	<servlet>
		<servlet-name>Controller</servlet-name>
		<servlet-class>board.controller.ControllerAction</servlet-class>
		<init-param>
			<param-name>propertyConfig</param-name>
			<param-value>CommandPro.properties</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>Controller</servlet-name>
		<url-pattern>*.do</url-pattern>
	</servlet-mapping>


</web-app>
